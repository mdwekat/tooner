{
  "name": "tooner",
  "version": "0.1.3",
  "description": "Token-efficient serialization for LLMs - Convert JSON/YAML/TOML to TOON format",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./json": {
      "import": {
        "types": "./dist/json.d.ts",
        "default": "./dist/json.js"
      },
      "require": {
        "types": "./dist/json.d.cts",
        "default": "./dist/json.cjs"
      }
    },
    "./yaml": {
      "import": {
        "types": "./dist/yaml.d.ts",
        "default": "./dist/yaml.js"
      },
      "require": {
        "types": "./dist/yaml.d.cts",
        "default": "./dist/yaml.cjs"
      }
    },
    "./toml": {
      "import": {
        "types": "./dist/toml.d.ts",
        "default": "./dist/toml.js"
      },
      "require": {
        "types": "./dist/toml.d.cts",
        "default": "./dist/toml.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "tooner": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src cli tests --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run lint && pnpm run typecheck && pnpm run build",
    "version": "pnpm changeset version",
    "release": "pnpm run build && changeset publish"
  },
  "keywords": [
    "toon",
    "serialization",
    "json",
    "yaml",
    "toml",
    "llm",
    "token-efficient",
    "encoding",
    "decoding"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdwekat/tooner.git"
  },
  "bugs": {
    "url": "https://github.com/mdwekat/tooner/issues"
  },
  "homepage": "https://github.com/mdwekat/tooner#readme",
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitest/coverage-v8": "^4.0.10",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "commander": "^14.0.2",
    "yaml": "^2.8.1"
  },
  "pnpm": {
    "overrides": {
      "glob@>=10.2.0 <10.5.0": ">=10.5.0"
    }
  }
}
